@model AdminHeaderLinksModel
@using Nop.Web.Models.Common;
@if (Model.IsCustomerImpersonated)
{
    <li>
    <a href="@Url.RouteUrl("Logout")" class="finish-impersonation" title="@T("Account.ImpersonatedAs.Finish.Hint")">
        
      @T("Account.ImpersonatedAs.Finish")
        </a>
    </li>
}
@if (Model.DisplayAdminLink)
{
    <li><a href="@Url.Content("~/admin")" class="ico-admin">@T("Account.Administration")</a>
    </li>
    <li class="divider"></li>
}
